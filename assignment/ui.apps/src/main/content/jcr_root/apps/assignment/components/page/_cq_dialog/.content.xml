<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="&#x9;&#xa;granite/ui/components/coral/foundation/container">
        <item jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="&#x9;&#xa;&#x9;&#xa;granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <metadata jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <typeOfProduct
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/selection"
                                fieldDescription="Select the type of product"
                                fieldLabel="Type of product"
                                name="./productType"
                                type="select">
                                <options jcr:primaryType="nt:unstructured">
                                    <car
                                        jcr:primaryType="nt:unstructured"
                                        type="Car"
                                        value="car"/>
                                    <bike
                                        jcr:primaryType="nt:unstructured"
                                        type="Bike"
                                        value="bike"/>
                                    <truck
                                        jcr:primaryType="nt:unstructured"
                                        type="Truck"
                                        value="truck"/>
                                </options>
                            </typeOfProduct>
                            <nameOfProduct
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/textfield"
                                fieldDescription="Enter the name of product"
                                fieldLabel="Name of product"
                                name="./productName"/>
                            <launchdate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/datepicker"
                                fieldDescription="select the launch date"
                                fieldLabel="Launch Date"
                                name="./launchdate"/>
                        </items>
                    </metadata>
                </items>
            </tabs>
        </item>
    </content>
</jcr:root>
